{
  "_type": "library",
  "_version": "HRKDSL",
  "collectionPaths": [
    {
      "basePath": "/assets/",
      "pathSpecifiers": [
        {
          "segment": "Extension",
          "specifier": "svg",
          "type": "Full"
        }
      ],
      "recursive": true,
      "transform": {
        "keyFormat": "${fileName}",
        "nameFormat": "${iconName}",
        "match": "(?<iconName>[^\\/]+)\\/(?<iconType>[^\\/]+)\\/(?<fileName>[^\\/]+_(?<iconSize>[\\d]+)_(?<iconStyle>[\\S]+))\\.svg$",
        "transforms": [
          {
            "identifier": "iconName",
            "operations": [ "Capitalize" ]
          }
        ],
        "dataTransforms": [
          {
            "method": "Copy",
            "params": {
              "from": "iconPath"
            },
            "variable": "pdf-file"
          },
          {
            "method": "Replace",
            "params": {
              "from": "svgFileExtension",
              "to": "pdfFileExtension"
            },
            "variable": "pdf-file"
          },
          {
            "method": "Replace",
            "params": {
              "from": "svgSubFolder",
              "to": "pdfSubFolder"
            },
            "variable": "pdf-file"
          }
        ],
        "dataVariables": [
          {
            "type": "Value",
            "valueType": "Text",
            "name": "pdf-file"
          },
          {
            "type": "Value",
            "valueType": "Text",
            "value": ".svg",
            "name": "svgFileExtension"
          },
          {
            "type": "Value",
            "valueType": "Text",
            "value": ".pdf",
            "name": "pdfFileExtension"
          },
          {
            "type": "Value",
            "valueType": "Text",
            "value": "/SVG/",
            "name": "svgSubFolder"
          },
          {
            "type": "Value",
            "valueType": "Text",
            "value": "/PDF/",
            "name": "pdfSubFolder"
          }
        ]
      }
    },
    {
      "collectionName": "Fluent Regular",
      "pathSpecifiers": [
        {
          "segment": "FileNameWithoutExtension",
          "specifier": "*_regular",
          "type": "WildCard"
        }
      ]
    },
    {
      "collectionName": "Fluent Filled",
      "pathSpecifiers": [
        {
          "segment": "FileNameWithoutExtension",
          "specifier": "*_filled",
          "type": "WildCard"
        }
      ]
    }
  ],
  "dataPaths": [
    {
      "dataPath": "~/_iconCloud/keywords.csv",
      "transformPaths": [
        "~/_iconCloud/keywords.transforms.json"
      ],
      "variablePaths": [
        "~/_iconCloud/keywords.variables.json"
      ]
    },
    {
      "dataPath": "~/_iconCloud/metaphors.csv",
      "transformPaths": [
        "~/_iconCloud/metaphors.transforms.json"
      ],
      "variablePaths": [
        "~/_iconCloud/metaphors.variables.json"
      ]
    }
  ],
  "iconType": "Svg",
  "lastUpdated": "2021-11-09T20:35Z",
  "libraryInfoKey": "3821b375b",
  "libraryName": "Fluent System Library",
  "libraryProperties": {
    "copyPreviewTypes" : "Svg",
    "copySize" : "24",
    "copyTypes" : "Svg",
    "downloadTypes" : "Svg,Pdf",
    "libraryIconSizes": "12px, 16px, 20px, 24px, 28px, 48px",
    "metadata-1-label" : "Keywords",
    "metadata-1-dataType" : "Keywords",
    "metadata-1-dataStyle" : "ClickablePill",
    "metadata-1-dataMapping" : "Icon:iconKeywords",
    "metadata-1-sorting" : "ByWeight",
    "metadata-2-label" : "Metaphors",
    "metadata-2-dataType" : "Keywords",
    "metadata-2-dataStyle" : "ClickablePill",
    "metadata-2-dataMapping" : "Icon:iconMetaphors",
    "metadata-2-sorting" : "None",

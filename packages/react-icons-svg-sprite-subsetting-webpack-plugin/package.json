{
  "name": "@fluentui/react-icons-svg-sprite-subsetting-webpack-plugin",
  "version": "0.0.0",
  "description": "Webpack plugin to subset or merge SVG sprite assets used by @fluentui/react-icons svg-sprite APIs.",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run test:atomic && npm run test:merged",
    "test:atomic": "webpack -c test/webpack.config.js",
    "test:merged": "SVG_SPRITE_MODE=merged webpack -c test/webpack.config.js",
    "build": "tsc -p ."
  },
  "engines": {
    "node": ">=14.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsoft/fluentui-system-icons.git",
    "directory": "packages/react-icons-svg-sprite-subsetting-webpack-plugin"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microsoft/fluentui-system-icons/issues"
  },
  "homepage": "https://github.com/microsoft/fluentui-system-icons/tree/main/packages/react-icons-svg-sprite-subsetting-webpack-plugin#readme",
  "devDependencies": {
    "@fluentui/react-icons": "*",
    "@types/node": "14",
    "typescript": "5.0.4",
    "webpack": "^5.72.0"
  },
  "peerDependencies": {
    "webpack": ">=5.0.0"
  },
  "files": [
    "lib/*"
  ]
}

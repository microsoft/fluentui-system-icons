{
  "name": "@fluentui/react-native-icons",
  "version": "2.0.306",
  "sideEffects": false,
  "main": "lib-cjs/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "description": "Fluent System Icons are a collection of familiar, friendly, and modern icons from Microsoft.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/fluentui-system-icons.git"
  },
  "scripts": {
    "clean": "git clean -fXd intermediate/ lib/ lib-cjs/ src/",
    "copy": "node ../../importer/generate.js --source=../../assets --dest=./intermediate --extension=svg --target=react",
    "convert:svg": "node ./convert-rn.js --source=./intermediate --dest=./src --native=true",
    "optimize": "svgo --config svgo.config.js --folder=./intermediate --precision=2 --quiet",
    "unfill": "node ./unfill-svg.js",
    "build:fonts-and-svg": "npm run copy && npm run optimize && npm run unfill",
    "build:generate-js-chunks": "npm run convert:svg",
    "build:js": "concurrently 'npm run build:cjs' 'npm run build:esm'",
    "build:cjs": "tsc --jsx react --module commonjs --outDir lib-cjs",
    "build:esm": "tsc --jsx react",
    "build": "npm run build:fonts-and-svg && npm run build:generate-js-chunks && npm run build:js"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.16.7",
    "@svgr/core": "^6.5.1",
    "@types/react": "^17.0.2",
    "cpy-cli": "^4.1.0",
    "glob": "^7.2.0",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "react": "^17.0.1",
    "react-native": "^0.68.0",
    "renamer": "^2.0.1",
    "svgo": "^2.8.0",
    "typescript": "~4.1.0",
    "yargs": "^14.0.0"
  },
  "dependencies": {
    "@types/react-native": "^0.68.0",
    "tslib": "^2.1.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0 <19.0.0",
    "react-native-svg": ">=12.5.0"
  },
  "files": [
    "lib/",
    "lib-cjs/"
  ],
  "exports": {
    ".": {
      "default": {
        "types": "./lib/index.d.ts",
        "import": "./lib/index.js",
        "require": "./lib-cjs/index.js"
      }
    }
  }
}

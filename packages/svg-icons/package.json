{
  "name": "@fluentui/svg-icons",
  "version": "1.1.50",
  "description": "Fluent System Icons are a collection of familiar, friendly and modern icons from Microsoft.",
  "main": "index.js",
  "scripts": {
    "clean": "shx rm -rf dist/*",
    "copy": "node ../../importer/generate.js --source=../../assets --dest=./dist --extension=svg",
    "optimize": "svgo --folder=./dist --precision=2 --disable=removeViewBox,mergePaths",
    "unfill": "replace '( fill=\"none\"| fill=\"#212121\")' '' ./dist/*.svg --silent",
    "build": "npm run copy && npm run optimize && npm run unfill",
    "prepublishOnly": "npm run build",
    "postpublish": "npm run clean"
  },
  "author": "Microsoft Inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsoft/fluentui-system-icons.git"
  },
  "devDependencies": {
    "replace": "^1.2.0",
    "shx": "^0.3.2",
    "svgo": "^1.3.2",
    "yargs": "^14.0.0"
  }
}

{
  "version": "1.1.166",
  "description": "Fluent System Icons are a collection of familiar, friendly, and modern icons from Microsoft.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsoft/fluentui-system-icons.git"
  },
  "scripts": {
    "clean": "shx rm -rf ./icons",
    "copy": "node generate_with_dirs.js --source=../../assets --dest=./icons --extension=svg",
    "rename": "renamer --find ic_fluent_ ./icons/*",
    "optimize": "svgo --folder=./icons/ --recursive --precision=2 --disable=removeViewBox,mergePaths",
    "unfill": "node unfill.js --path=./icons/",
    "build": "npm run copy && npm run rename && npm run optimize && npm run unfill"
  },
  "devDependencies": {
    "lodash": "4.17.21",
    "renamer": "^2.0.1",
    "replace": "^1.2.0",
    "shx": "^0.3.2",
    "svgo": "^1.3.2",
    "yargs": "^14.2.3"
  }
}

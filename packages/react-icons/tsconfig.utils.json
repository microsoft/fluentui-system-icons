{
  "compilerOptions": {
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "noEmit": true,
    "module": "commonjs",
    "target": "es2020",
    "allowJs": true,
    "checkJs": true,
    "types": ["node"],
    "skipLibCheck": true,
    "strictNullChecks": true,
    "pretty": true
  },
  "include": [
    "convert.js",
    "convert-font.js",
    "convert.utils.js",
    "convert-font.utils.js",
    "merge-metadata.js",
    "metadata.utils.js",
    "sprite.writer.js",
    "per-icon.writer.js",
    "chunking-utils.js",
    "build.js",
    "deprecated-atoms.js",
  ]
}
